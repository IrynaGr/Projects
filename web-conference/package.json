{
  "name": "Web-Conferencing",
  "version": "0.1.0",
  "author": "Iryna",
  "dependencies": {
    "create-react-class": "^15.6.2",
    "file-saver": "^1.3.3",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-autocomplete": "^1.7.2",
    "react-bootstrap": "^0.32.1",
    "react-datepicker": "^0.64.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.3",
    "react-filters": "^0.3.1",
    "react-form-with-constraints": "^0.7.0",
    "react-history": "^0.18.2",
    "react-modal": "^3.1.11",
    "react-moment": "^0.6.8",
    "react-navigation": "^1.0.0-beta.27",
    "react-page-click": "^4.0.2",
    "react-pager": "^1.3.3",
    "react-router": "^4.2.0",
    "react-router-component": "^0.39.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-simplewebrtc": "0.0.18",
    "react-textarea-autosize": "^5.2.1",
    "react-validate-form": "^1.0.6",
    "simplewebrtc": "^3.0.1",
    "state-transitions": "^2.1.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/file-saver": "^1.3.0",
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.18.8",
    "browserify": "^14.4.0",
    "concurrently": "^3.4.0",
    "cssmin": "^0.4.3",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^7.4.0",
    "imagemin-cli": "^3.0.0",
    "kss": "^3.0.0-beta.18",
    "michelangelo": "^0.6.1",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "postcss-cli": "^2.6.0",
    "rimraf": "^2.6.1",
    "uglify-js": "^2.8.9"
  },
  "scripts": {
    "clean": "rimraf dist",
    "css": "node-sass --output-style compressed -o dist/css src/css/index.scss",
    "kss": "kss --config kss.config.json",
    "autoprefixer": "postcss -u autoprefixer --autoprefixer.browsers 'last 2 versions' -r dist/css/*",
    "cssmin": "cat dist/css/* | cssmin > dist/css/styles.min.css",
    "lint": "eslint src/js",
    "compile": "babel --presets es2015,stage-0,react -d dist src/",
    "bundle": "browserify dist/js/*.js > dist/js/bundle.js",
    "uglify": "uglifyjs dist/js/bundle.js -m -c -o dist/js/scripts.min.js",
    "watch:css": "onchange 'src/css/**/*.scss' -- npm run build:css",
    "watch:js": "onchange 'src/js/**/*.js' -- npm run build:js",
    "watch:all": "npm-run-all --parallel watch:css watch:js",
    "build:css": "npm run kss && npm run css && npm run autoprefixer && npm run cssmin",
    "build:img": "imagemin src/img/* -o dist/img",
    "build:js": "npm run lint && npm run compile && npm run bundle",
    "build": "npm run clean && npm-run-all --parallel build:css build:img build:js",
    "serve": "browser-sync start --server --files 'dist' --logPrefix 'Bootcamp' --logConnections",
    "start": "npm run build && npm-run-all --parallel serve watch:all"
  }
}
